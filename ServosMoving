#include <Servo.h>
//The Servos
Servo L;
Servo R;
Servo CR;

//int pL = 180;
//int pR = 180;

void setup(){
  L.attach(10);
  R.attach(9);
  //CR.attach(21);
}//end setup

void loop(){
  for(int x = 0;x == 3;x++){
  L.writeMicroseconds(2000);
  R.writeMicroseconds(1800);
  //CR.write(90);
  };
  delay(100);
}
